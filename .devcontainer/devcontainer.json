{
  "name": "Existing Dockerfile",
  "context": "..",
  "dockerFile": "../Dockerfile",
  "customizations": {
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "astro-build.astro-vscode",
        "bradlc.vscode-tailwindcss"
      ]
    }
  },
  "workspaceFolder": "/root/projects/awesome",
  "workspaceMount": "source=${localWorkspaceFolder},target=/root/projects/awesome,type=bind"
}
