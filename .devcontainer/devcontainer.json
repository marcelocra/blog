{
  "name": "Existing Dockerfile",
  "context": "..",
  "dockerFile": "../Dockerfile",
  "customizations": {
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "astro-build.astro-vscode",
        "bradlc.vscode-tailwindcss"
      ]
    }
  },
  "workspaceMount": "source=${localWorkspaceFolder},target=/workspaces/personal/${localWorkspaceFolderBasename},type=bind",
  "workspaceFolder": "/workspaces/personal/${localWorkspaceFolderBasename}"
}
